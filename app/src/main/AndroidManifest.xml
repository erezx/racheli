<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.erez.racheli2">
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.example.erez.racheli2.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.erez.racheli2.DetailActivity"
            android:parentActivityName="com.example.erez.racheli2.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.erez.racheli2.MainActivity" />
        </activity>
        <activity
            android:name="com.example.erez.racheli2.SongDetailActivity"
            android:parentActivityName="com.example.erez.racheli2.MainActivity">
            <meta-data
                android:name="a


                ndroid.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name="com.example.erez.racheli2.SampleDetailActivity"
            android:parentActivityName="com.example.erez.racheli2.MainActivity">
            <meta-data
                android:name="a


                ndroid.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
    </application>

</manifest>